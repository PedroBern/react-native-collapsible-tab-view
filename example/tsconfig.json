{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "paths": {
      "react-native-collapsible-tab-view": [
        "../src/index"
      ]
    },
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "rootDirs": [
      "src"
    ]
  },
  "extends": "expo/tsconfig.base"
}
